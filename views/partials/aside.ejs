<div id="containmain">
<% if(!session.username){ %>
	<aside>
		<form action="<%= postUrl %>" method="post">
 			<label for="field_username">Username: </label>
  			<input type="text" id="field_username" name="username" value="">

  			<label for="field_password">Password: </label>
  			<input type="password" id="field_password" name="password" value="">

  			<button type="submit">Login!</button>
		</form>
		<p> Not a member yet? <a href="/mypicture/register">Join Now!</a></p>
	</aside>
<% } else {	%>
	<aside>
		<p> Hey, <%= session.username %>! Good to see you!</p>
		<a href='/mypicture/logout'>Logout</a>	
	</aside>

<% } %>
